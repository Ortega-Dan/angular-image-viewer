{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "ng serve",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "/./*": "${webRoot}/*",
                "/src/*": "${webRoot}/*",
                "/*": "*",
                "/./~/*": "${webRoot}/node_modules/*",
                "webpack:///ng://angular-x-image-viewer/lib/*": "${workspaceFolder}/projects/angular-image-viewer/src/lib/*"
            }
        }
    ]
}